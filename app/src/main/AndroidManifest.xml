<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.seguridadsocialmanual">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Manual de usuario"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Manual_Page"></activity>
        <activity
            android:name=".AcercaDe"
            android:theme="@style/AppTheme.TemaPopUp" />
        <activity android:name=".Menu" />
        <activity android:name=".PrimeraVentana" />
        <activity
            android:name=".CambiarIdioma"
            android:parentActivityName=".PrimeraVentana"
            android:theme="@style/AppTheme.TemaPopUp" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>